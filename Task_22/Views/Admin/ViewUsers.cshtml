<div class="row">
    <div class="col-12 position-left">
        <ul class="list-btn list-reset">
            <li class="item-btn">
                <button class="button" onclick="setUnchecked()">
                    Выделить все
                </button>
            </li>
            <li class="item-btn">
                <button class="button" onclick="onDelete()">
                    Удалить
                </button>
            </li>
        </ul>
    </div>
</div>
<div class="row">
    <div class="col-12 ">
        <table class="box__table">
            <tr class="table__row table__row--one-row">
                <th class="cell cell-mid">
                    Имя Пользователя
                </th>
                <th class="cell">
                    Роль
                </th>
                <th class="cell cell-slim">
                </th>
            </tr>
            @foreach (var item in ViewBag.DataUsers)
            {
                <tr class="table__row row-target table__row--not-even">
                    <td class="cell cell-name cell-mid">
                        @item.Name
                    </td>
                    <td class="cell">
                        @item.Role
                    </td>
                    <td class="cell cell-slim">
                        <input type="checkbox" class="checkbox-cell">
                    </td>
                </tr>
            }
        </table>
    </div>
</div>
<script src="~/libs/jquery.min.js"></script>
<script src="~/js/deleteCheckbox2.js"></script>